<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ArcProgress">
        <attr name="arc_progress" format="float"/>
        <attr name="arc_angle" format="float"/>
        <attr name="arc_stroke_width" format="dimension"/>
        <attr name="arc_max" format="integer"/>

        <attr name="arc_unfinished_color" format="color"/>
        <attr name="arc_finished_color" format="color"/>

        <attr name="arc_text_size" format="dimension"/>
        <attr name="arc_text_color" format="color"/>

        <attr name="arc_suffix_text" format="string"/>
        <attr name="arc_suffix_text_size" format="dimension"/>
        <attr name="arc_suffix_text_padding" format="dimension"/>
        <attr name="arc_suffix_text_font" format="reference"/>

        <attr name="arc_bottom_text" format="string"/>
        <attr name="arc_bottom_text_size" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="PhoneInput">
        <attr name="text" format="string"/>
        <attr name="text_title" format="string"/>
        <attr name="text_hint" format="string"/>
        <attr name="text_nocode" format="string"/>
    </declare-styleable>

    <declare-styleable name="OTPInput">
        <attr name="otp_one" format="string"/>
        <attr name="otp_two" format="string"/>
        <attr name="otp_three" format="string"/>
        <attr name="otp_four" format="string"/>
        <attr name="otp_five" format="string"/>
        <attr name="otp_six" format="string"/>
        <attr name="otp_error" format="string"/>
    </declare-styleable>

    <declare-styleable name="ImageProfile">
        <attr name="url_profile" format="string"/>
        <attr name="progress_profile" format="boolean"/>
    </declare-styleable>
</resources>